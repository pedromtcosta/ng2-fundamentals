{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "class-name": true,
        "quotemark": [
            true,
            "single"
        ] ,
        "whitespace": [
            false
        ],
        "no-trailing-whitespace": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}